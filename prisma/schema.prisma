
generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Url {
  id         Int      @id @default(autoincrement())
  longUrl    String   @unique
  shortCode  String   @unique @db.VarChar(8)
  hitCount   Int      @default(0)
  expiresAt  DateTime?
  createdAt  DateTime @default(now())
}